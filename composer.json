{
    "name": "madbob/laravel-log2rss",
    "description": "Laravel logs accessible in RSS",
    "license": "GPL-3.0-or-later",
    "keywords": ["laravel", "logs", "rss"],
    "authors": [
        {
            "name": "Roberto Guido",
            "email": "bob@linux.it"
        }
    ],
    "require": {
        "rap2hpoutre/laravel-log-viewer": "^2.0",
        "laravelium/feed": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "MadBob\\LaravelLog2Rss\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MadBob\\LaravelLog2Rss\\Log2RssServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
